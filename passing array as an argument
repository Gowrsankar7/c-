#include<iostream>
using namespace std;

void displayarray(const int *arr,int size)
  { 
    for(int i=0;i<size;i++)
     {
       cout<<arr[i];
     }
    cout<<endl;
  }
  
  int main()
  {
      int arr[5]={1,2,3,4,5};
      cout<<"before calling"<<endl;
       for(int i=0;i<5;i++)
       {
           cout<<arr[i]<<"";
       }
       cout<<endl;
      cout<<"after calling"<<endl;
  displayarray(arr,5);
  //cout<<arr[5]<<"";
  
  return 0;
  }

output:
before calling
12345
after calling
12345
